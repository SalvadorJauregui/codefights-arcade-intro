String alphabeticShift(String inputString) {
    StringBuilder sb = new StringBuilder(inputString);
        for (int i = 0; i < sb.length(); i++) {
            if (sb.charAt(i) == 'z')
                sb.setCharAt(i, 'a');
            else
                sb.setCharAt(i, (char)(sb.charAt(i) + 1));
        }
    return sb.toString();
}
