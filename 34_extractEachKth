int[] extractEachKth(int[] inputArray, int k) {    
    int[] newArray = new int[inputArray.length - (inputArray.length/k)];
        
    for (int i = 0, j = 0; i < inputArray.length; i++){
        if (((i+1) % k) != 0){
            newArray[j++] = inputArray[i];            
        }
    }    
    return newArray;
}
